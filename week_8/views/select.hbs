<h1>{{ title }}</h1>

<table>
    <tr>                     {{!-- 행 --}}
        <td>Fname</td>       {{!-- 열 --}}
        <td>Minit</td>
        <td>Lname</td>
        <td>Ssn</td>
        <td>Bdate</td>
        <td>Address</td>
        <td>Sex</td>
        <td>Salary</td>
        <td>Super_ssn</td>
        <td>Dno</td>
    </tr>
    {{#each employee}}       {{!-- each 시작, employee 반복 --}}
    <tr> 
        <td>{{Fname}}</td>
        <td>{{Minit}}</td>
        <td>{{Lname}}</td>
        <td>{{Ssn}}</td>
        <td>{{Bdate}}</td>
        <td>{{Address}}</td>
        <td>{{Sex}}</td>
        <td>{{Salary}}</td>
        <td>{{Super_ssn}}</td>
        <td>{{Dno}}</td>
    </tr>
    {{/each}}                {{!-- each 끝, 데이터가 있을때마다 행 추가해서 값을 입력 --}}
</table>

